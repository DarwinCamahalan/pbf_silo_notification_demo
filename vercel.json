{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/send-notification",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/send-notification.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    }
  ]
}
